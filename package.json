{
  "name": "mono",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npx eslint . --fix --max-warnings 0",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint:styles": "npx stylelint ./**/*.scss --fix",
