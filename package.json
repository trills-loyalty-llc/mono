{
  "name": "mono",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npx eslint . --fix --max-warnings 0",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint:styles": "npx stylelint ./**/*.scss --fix",
    "format": "npx prettier . --write",
    "prepare": "npx husky",
    "cypress": "npx cypress open",
    "swagger": "npx swagger-typescript-api -p ./swagger/swagger.json -o ./src/data -n ApiClient.ts --axios"
  },
  "dependencies": {
    "bulma": "^1.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "sass": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  }
}
