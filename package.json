    "jsdom": "^27.4.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "sass": "^1.83.0",
    "storybook": "^10.1.11",
    "stylelint": "^17.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.2",
